<h1>Test</h1>
<form method="post" action="/test">
  <div class="form-group">
    <label for="question">Question</label>
    <input type="text" class="form-control" name="q1[question]" id="question" placeholder="Your question">
  </div>

  <div class="form-group">
    <label for="answer">Answer</label>
    <input type="text" class="form-control" name="q1[answer1]" id="answer" placeholder="Your answer">
  </div>

  <div class="form-group">
    <button type="submit" class="btn btn-lg btn-block btn-default">Submit</button>
  </div>
</form>
